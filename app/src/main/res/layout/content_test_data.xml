<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.lbconsulting.a1list.activities.TestDataActivity"
    tools:showIn="@layout/activity_test_data">


    <Button
        android:id="@+id/btnUploadAttributes"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0dp"
        android:padding="25dp"
        android:text="Upload Attributes" />

    <Button
        android:id="@+id/btnRetrieveAttributes"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        android:padding="25dp"
        android:text="Retrieve Attributes" />

    <Button
        android:id="@+id/btnUploadListTitles"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0dp"
        android:padding="25dp"
        android:text="Upload List Titles" />


    <Button
        android:id="@+id/btnRetrieveListTitles"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        android:padding="25dp"
        android:text="Retrieve List Titles" />

    <Button
        android:id="@+id/btnUploadListItems"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="0dp"
        android:padding="25dp"
        android:text="Upload List Items" />

    <Button
        android:id="@+id/btnRetrieveListItems"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        android:padding="25dp"
        android:text="Retrieve List Items" />


</LinearLayout>
